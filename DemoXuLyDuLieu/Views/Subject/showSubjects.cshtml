@model List<DemoXuLyDuLieu.Models.Subject>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>showSubjects</title>
    <link rel="stylesheet" href="~/Content/bootstrap.css" />

</head>
<body>
    <table class="table">
        <tr>
            <th>Mã môn học</th>
            <th>Tên môn học</th>
            <th>Action</th>
        </tr>
        <tr>
            @foreach (var item in Model)
            {
            <tr>
                <td>@item.SubjectID</td>
                <td>@item.SubjectName</td>
                <td>
                    <a  href="~/Subject/deleteSubject/@item.SubjectID" type="button" class="btn btn-success">Xoá</a>
                    <a href="~/Subject/updateSubject/@item.SubjectID" type="button" class="btn btn-success">Sửa</a>

                </td>

            </tr>
             }
         </tr>
    </table>
    <div>
        <a href="~/Subject/addSubject" type="button" class="btn btn-success"> Thêm</a>
    </div>
</body>
</html>
